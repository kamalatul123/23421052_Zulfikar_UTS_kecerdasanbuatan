# Query the knowledge base
person = "Hadi"
relation = "kakek dari"
related_people = kb.get_related_people(person, relation)

if related_people:
    print(f"{person} {relation} {', '.join(related_people)}")
else:
    print(f"No information found about {person} {relation}")
